{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens":"off",
    "require-jsdoc":"off",
    "linebreak-style": ["error", "windows"],
    "no-trailing-spaces": "off",
    "comma-dangle": "off",
    "padded-blocks": "off",
    "quotes": "off",
    "indent": "off",
    "operator-linebreak": "off"
  },
  "extends": [
    "google"
  ]
}